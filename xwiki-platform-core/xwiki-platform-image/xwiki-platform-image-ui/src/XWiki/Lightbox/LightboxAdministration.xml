  <content>{{velocity output="false"}}&#xd;
#macro (stripHTMLMacro $displayOutput)&#xd;
  $stringtool.removeEnd($stringtool.removeStart($displayOutput, '{{html clean="false" wiki="false"}}'), '{{/html}}')&#xd;
#end&#xd;
#macro (showProperty $propertyName)&#xd;
  &lt;dl>&#xd;
    &lt;dt>&#xd;
      #set ($propertyClass = $xclass.get($propertyName))&#xd;
      &lt;label#if ($editing) for="${lightboxConfigClassName}_0_${propertyName}"#end>&#xd;
        $services.localization.render("lightbox.admin.${propertyName}.label")&#xd;
      &lt;/label>&#xd;
      #set ($hint = $propertyClass.hint)&#xd;
      #if ("$!hint" == '')&#xd;
        #set ($hint = $services.localization.render("lightbox.admin.${propertyName}.hint"))&#xd;
      #end&#xd;
      &lt;span class="xHint">$escapetool.xml($hint)&lt;/span>&#xd;
    &lt;/dt>&#xd;
    #set ($displayOutput = $lightboxConfigDoc.display($propertyName, $mode))&#xd;
    &lt;dd>#stripHTMLMacro($displayOutput)&lt;/dd>&#xd;
  &lt;/dl>&#xd;
#end&#xd;
{{/velocity}}&#xd;
&#xd;
{{velocity}}&#xd;
#set ($lightboxConfigClassName = 'XWiki.Lightbox.LightboxConfigurationClass')&#xd;
#set ($lightboxConfigDoc = $xwiki.getDocument('XWiki.Lightbox.LightboxConfiguration'))&#xd;
#set ($discard = $lightboxConfigDoc.use($lightboxConfigClassName))&#xd;
#set ($lightboxConfigClass = $xwiki.getDocument($lightboxConfigClassName))&#xd;
#set ($xclass = $lightboxConfigClass.getxWikiClass())&#xd;
#set ($editing = $xcontext.action == 'edit' || $xcontext.action == 'admin')&#xd;
#set ($mode = "#if ($editing)edit#{else}view#end")&#xd;
{{html clean="false"}}&#xd;
#if ($xcontext.action == 'admin')&#xd;
  &lt;form action="$lightboxConfigDoc.getURL('save')" class="xform">&#xd;
    &lt;div class="hidden">&#xd;
      &lt;input type="hidden" name="form_token" value="$!escapetool.xml($services.csrf.token)"/>&#xd;
    &lt;/div>&#xd;
#else&#xd;
  &lt;div class="xform">&#xd;
#end&#xd;
  #showProperty('isLightboxEnabled')&#xd;
#if ($xcontext.action == 'admin')&#xd;
  &lt;p>&#xd;
    &lt;input type="submit" class="btn btn-primary" name="action_saveandcontinue"&#xd;
      value="$escapetool.xml($services.localization.render('save'))"/>&#xd;
  &lt;/p>&#xd;
&lt;/form>&#xd;
#else&#xd;
&lt;/div>&#xd;
#end&#xd;
{{/html}}&#xd;
{{/velocity}}</content>
