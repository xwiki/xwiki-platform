  "devDependencies": {
    "@microsoft/api-extractor": "catalog:",
    "@types/jquery": "catalog:",
    "@vue/test-utils": "catalog:",
    "@xwiki/platform-dev-config": "workspace:*",
    "@xwiki/platform-livedata-componentstore": "workspace:*",
    "@xwiki/platform-tool-tsconfig": "workspace:*",
    "eslint": "catalog:",
    "flush-promises": "catalog:",
    "jquery": "catalog:",
    "lodash-es": "catalog:",
    "sinon": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-i18n": "catalog:",
    "vue-tsc": "catalog:"
  }
