&lt;form class="xform"&gt;
  &lt;dl&gt;
    &lt;dt&gt;
      &lt;label for="testTitle"&gt;Title&lt;/label&gt;
    &lt;/dt&gt;
    &lt;dd&gt;
      &lt;input type="text" id="testTitle" name="title" value="Form fields" /&gt;
    &lt;/dd&gt;
    &lt;dt&gt;
      &lt;label&gt;
        &lt;input type="checkbox" name="enabled" /&gt;
        Enabled
      &lt;/label&gt;
    &lt;/dt&gt;
    &lt;dd&gt;&lt;/dd&gt;
    &lt;dt&gt;
      &lt;label&gt;Color&lt;/label&gt;
    &lt;/dt&gt;
    &lt;dd&gt;
      &lt;label&gt;
        &lt;input type="radio" name="color" value="blue" /&gt; Blue
      &lt;/label&gt;
      &lt;label&gt;
        &lt;input type="radio" name="color" value="yellow" /&gt; Yellow
      &lt;/label&gt;
      &lt;label&gt;
        &lt;input type="radio" name="color" value="red" /&gt; Red
      &lt;/label&gt;
    &lt;/dd&gt;
    &lt;dt&gt;
      &lt;label for="testCity"&gt;City&lt;/label&gt;
    &lt;/dt&gt;
    &lt;dd&gt;
      &lt;select id="testCity" name="city"&gt;
        &lt;option&gt;Iasi&lt;/option&gt;
        &lt;option&gt;Paris&lt;/option&gt;
      &lt;/select&gt;
    &lt;/dd&gt;
    &lt;dt&gt;
      &lt;label for="testGenre"&gt;Genre&lt;/label&gt;
    &lt;/dt&gt;
    &lt;dd&gt;
      &lt;select id="testGenre" name="genre" multiple&gt;
        &lt;option&gt;Comedy&lt;/option&gt;
        &lt;option&gt;Drama&lt;/option&gt;
        &lt;option&gt;Romance&lt;/option&gt;
      &lt;/select&gt;
    &lt;/dd&gt;
    &lt;dt&gt;
      &lt;label for="testDescription"&gt;Description&lt;/label&gt;
    &lt;/dt&gt;
    &lt;dd&gt;
      &lt;textarea id="testDescription" name="description"&gt;this is a test&lt;/textarea&gt;
    &lt;/dd&gt;
  &lt;/dl&gt;
  &lt;div&gt;
    &lt;button name="submit" value="submit"&gt;Submit&lt;/button&gt;
  &lt;/div&gt;
&lt;/form&gt;
{{/html}}</content>
