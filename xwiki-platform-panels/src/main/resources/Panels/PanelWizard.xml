          window.allPanelsPlace[$counter]['fullname'] = '$item';
          #set($issetleft = 0)
          #set($counterleft = 0)
          #set($issetright = 0)
          #set($counterright = 0)
          #foreach($element in $leftPanels)
            #if($item == $element)
