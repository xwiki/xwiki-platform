{
  "name": "@cristal/skin",
  "version": "0.7.0",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "directory": "skin",
    "url": "https://github.com/xwiki-contrib/cristal/"
  },
  "type": "module",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "pnpm compile",
    "clean": "rimraf dist",
    "compile": "vue-tsc --project tsconfig.json --noEmit && vite build",
    "lint": "eslint \"./src/**/*.{ts,tsx,vue}\" --max-warnings=0",
    "prepack": "pnpm build",
    "test": "echo 'no test'"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cristal/api": "workspace:*",
    "@cristal/icons": "workspace:*",
    "inversify": "6.0.2",
    "marked": "12.0.2",
    "reflect-metadata": "0.2.2",
    "vue": "3.4.27",
    "vue-i18n": "9.13.1",
    "vue-router": "4.3.2",
    "vuetify": "3.6.7"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.0.4",
    "rimraf": "5.0.7",
    "vite": "5.2.11",
    "vite-plugin-inspect": "0.8.4",
    "vite-plugin-vuetify": "2.0.3",
    "vue-tsc": "2.0.19"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.es.js",
        "require": "./dist/index.umd.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.es.js",
    "types": "./dist/index.d.ts"
  }
}
