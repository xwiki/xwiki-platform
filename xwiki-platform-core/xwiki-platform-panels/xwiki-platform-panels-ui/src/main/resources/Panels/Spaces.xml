    <property>
      <content>{{velocity}}
#panelheader($msg.get('xe.panels.spaces'))
#set ($spaces = $xwiki.spaces)
#foreach ($space in $spaces)
  #if ($hasAdmin || ($xwiki.hasAccessLevel('view', "${space}.WebHome") &amp;&amp; !$blacklistedSpaces.contains($space)))
    #if ($foreach.index &gt; 0)
      (% class="pitemseparator" %) ~| (%%)##
    #end
    (% class="panelitem#if ($space == $doc.space) currentspace#end" %)[[$space&gt;&gt;${space}.WebHome]](%%)##
  #end
#end
#panelfooter()
{{/velocity}}</content>
