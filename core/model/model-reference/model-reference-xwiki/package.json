    "typescript": "5.6.3",
    "vite": "6.0.3",
    "vitest": "2.1.8",
    "vitest-mock-extended": "2.0.2",
    "vue": "3.5.13"
  },
  "publishConfig": {
    "exports": {
      ".": {
