  <content>{{velocity output="false"}}
#macro(saveGoogleAccount $account)
	#set($analyticsConfigDoc = $xwiki.getDocument('XWiki.GoogleAnalyticsConfig'))
	#set($analyticsCodeDoc = $xwiki.getDocument('XWiki.GoogleAnalyticsCode'))
	#set($analyticsObj = $analyticsConfigDoc.getObject('XWiki.GoogleAnalyticsCode', true))
	#set($jsObj = $analyticsCodeDoc.getObject('XWiki.JavaScriptExtension'))
	#set($discard = $analyticsObj.set('account', $!account))
	$analyticsConfigDoc.save()
	#if ("$!jsObj" != "") 
		#if ("$!account" != "")
			#set($discard = $jsObj.set('use', 'always'))
		#else
			#set($discard = $jsObj.set('use', 'onDemand'))	
		#end
		$analyticsCodeDoc.save()
	#end
#end ##macro

#macro(displayAnalyticsStatus)
	#set($analyticsCodeDoc = $xwiki.getDocument('XWiki.GoogleAnalyticsCode'))
	#set($jsObj = $analyticsCodeDoc.getObject('XWiki.JavaScriptExtension'))
	#if("$!jsObj" != "")
		#if ("$!analyticsCodeDoc.getValue('use', $jsObj)" == 'always')
			{{info}}$msg.get("admin.analytics.running"){{/info}}
		#else
			{{info}}$msg.get("admin.analytics.notrunning"){{/info}}
		#end
	#else
		{{error}}$msg.get("admin.analytics.noscript"){{/error}}
	#end
#end ##macro
{{/velocity}}

{{velocity}}
#if ($doc.name != 'XWikiPreferences' &amp;&amp; $xcontext.action == 'view')
$response.sendRedirect($xwiki.getURL('XWiki.XWikiPreferences', 'admin', 'section=analytics')) 
#end
#set($analyticsConfigDoc = $xwiki.getDocument('XWiki.GoogleAnalyticsConfig'))
#set($analyticsCodeDoc = $xwiki.getDocument('XWiki.GoogleAnalyticsCode'))##
#set($analyticsObj = $analyticsConfigDoc.getObject('XWiki.GoogleAnalyticsCode', true))##
#set($analyticsCodeClass = $analyticsCodeDoc.xWikiClass)##
#set($accountPropertyClass = $analyticsCodeClass.account)##
#if($hasAdmin)##
#if($request.action == 'save')##
#saveGoogleAccount($request.get('analytics_account'))##
#end## save action
#else## !hasAdmin
{{error}}$msg.get('needadminrights').{{/error}}
#end##
#displayAnalyticsStatus()

{{html}}
&lt;form action="$xwiki.requestURL" enctype="multipart/form-data" method="post" class="xform half"&gt;
	&lt;dl&gt;
		&lt;dt&gt;
			&lt;label for="analytics_account"&gt;$!accountPropertyClass.prettyName&lt;/label&gt;
			&lt;span class="xHint"&gt;$msg.get("admin.analytics.sectiondesc")&lt;/span&gt;
		&lt;/dt&gt;
		&lt;dd&gt;
			$analyticsConfigDoc.displayEdit($accountPropertyClass, 'analytics_', $analyticsObj)
		&lt;/dd&gt;
	&lt;/dl&gt;
	&lt;p&gt;
		&lt;input type="hidden" name="action" value="save"/&gt;
		&lt;span class="buttonwrapper"&gt;
			&lt;input type="submit" value="${msg.get('save')}" class="button" /&gt;
		&lt;/span&gt;
	&lt;/p&gt;
&lt;/form&gt;
{{/html}}

{{/velocity}}</content>
