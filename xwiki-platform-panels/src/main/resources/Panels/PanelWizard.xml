      #set($ajaxdoc = $xwiki.getDocument("Panels.AjaxEditor"))
      #if($request.place)
        #set($ajaxurl = $xwiki.getDocument("Panels.PanelLayoutUpdate").getURL("view", "xpage=plain&amp;place=${request.place}&amp;prefsdoc=${currentDoc}"))
