            #set($arrayList = $categoryMap.get($obj.category))
            #if($arrayList)
              #set($discard = $categoryMap.get($obj.category).add($item))
              ## TODO: uncategorized?
            #end
          #end
        #end
      #end
      #set($panelsPerRow = 3)
