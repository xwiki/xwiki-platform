#macro (showExtensions)
  #foreach ($extension in $services.uix.getExtensions('org.xwiki.platform.docdoesnotexist', {'sortByParameter' : 'order'}))
    $services.rendering.render($extension.execute(), 'html/5.0')
  #end
#end
