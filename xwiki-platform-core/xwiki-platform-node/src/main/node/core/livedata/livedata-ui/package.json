  },
  "bugs": {
    "url": "https://jira.xwiki.org/projects/XWIKI/"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.umd.js"
      }
    }
  },
  "main": "./dist/index.es.js",
  "files": [
    "dist",
    "src",
    "!src/**/*.spec.*",
    "!src/**/*.test.*",
    "!src/**/__tests__"
  ],
  "scripts": {
    "api-extractor:ci": "api-extractor run",
    "api-extractor:local": "api-extractor run --local",
    "build": "vite build",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.{ts,tsx,vue}\" --max-warnings=0",
    "test": "vitest --run"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@xwiki/platform-livedata-api": "workspace:*",
    "dompurify": "catalog:",
    "eventemitter3": "catalog:",
    "lz-string": "catalog:",
    "tippy.js": "catalog:",
    "u-node": "catalog:",
    "vue-tippy": "catalog:",
