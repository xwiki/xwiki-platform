#macro(displayExtensionLink $extension)
  #if ($extension.name)
    #set ($extensionName = $extension.name)
  #else
    #set ($extensionName = $extension.id.id)
  #end
  #set ($extensionName = $services.rendering.escape("${extensionName.trim()} ${extension.id.version.value}", 'xwiki/2.1'))
  #if ($services.security.authorization.hasAccess('ADMIN', "wiki:${xcontext.database}"))
    #set ($extensionURL = $xwiki.getURL('XWiki.XWikiPreferences', 'admin', $escapetool.url({'section' : 'XWiki.Extensions', 'extensionId' : $extension.id.id, 'extensionVersion' : $extension.id.version.value})))
    [[$extensionName&gt;&gt;path:$extensionURL||class="extension-link"]]
  #else
    $extensionName
  #end
#end
## Search for extensions providing the missing macro
#set ($macroId = $renderingerror.message.argumentArray[0])
#set ($extensionQuery = $services.extension.newQuery('').addFilter('components__org.xwiki.rendering.macro.Macro', $macroId, 'EQUAL'))
#set ($extensions = $services.extension.index.repository.search($extensionQuery))
#if ($extensions.size &gt; 0)
  {{translation key="extension.rendering.macro.error.unknownMacro.followingMacros" parameters="$macroId"/}}#if (!$renderingerror.inline)
    ## Make sure the boxes are standalone

  #end
  #foreach($extension in $extensions)
    #if (!$renderingerror.inline)
      * ##
    #else
      ~ ##
    #end
    #displayExtensionLink($extension)
  #end
#end
{{/velocity}}</content>
