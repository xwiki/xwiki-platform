      <content>{{velocity}}
#set ($lightboxConfigDoc = $xwiki.getDocument('XWiki.Lightbox.LightboxConfiguration'))
#set ($lightboxConfigObj = $lightboxConfigDoc.getObject('XWiki.Lightbox.LightboxConfigurationClass'))
#if ($lightboxConfigObj.getValue('isLightboxEnabled').equals(1))
  #set ($discard = $xwiki.linkx.use($services.webjars.url('org.xwiki.contrib:webjar-blueimp-gallery', 'css/blueimp-gallery.min.css'), {'type': 'text/css', 'rel': 'stylesheet'}))
  #set ($discard = $xwiki.jsx.use('XWiki.Lightbox.WebHome'))
  #set ($discard = $xwiki.ssx.use('XWiki.Lightbox.WebHome'))
  {{html clean="false"}}
  &lt;!-- The Gallery lightbox dialog. -->
  &lt;div
    id="blueimp-gallery"
    class="blueimp-gallery"
    aria-label="image gallery"
    aria-modal="true"
    role="dialog"
  >
    &lt;div class="slides" aria-live="polite">&lt;/div>
    &lt;h3 class="title">&lt;/h3>
    &lt;a
      class="prev" aria-controls="blueimp-gallery"
      aria-label="previous slide" aria-keyshortcuts="ArrowLeft"
    >&lt;/a>
    &lt;a
      class="next" aria-controls="blueimp-gallery"
      aria-label="next slide" aria-keyshortcuts="ArrowRight"
    >&lt;/a>
    &lt;div class="controls btn-group">
      &lt;a
        class="btn fullscreen" id="lightboxFullscreen" aria-controls="blueimp-gallery"
        aria-label="fullscreen" aria-keyshortcuts="Fullscreen"
      >$services.icon.renderHTML('arrows')&lt;/a>
      &lt;a
        class="btn download" id="lightboxDownload" aria-controls="blueimp-gallery"
        aria-label="download" aria-keyshortcuts="Download"
      >$services.icon.renderHTML('download')&lt;/a>
      &lt;a
        class="btn autoPlay" aria-controls="blueimp-gallery" aria-label="autoPlay slideshow"
        aria-keyshortcuts="Space" aria-pressed="false"
      >$services.icon.renderHTML('play')&lt;/a>
      &lt;a
        class="btn escape" aria-controls="blueimp-gallery"
        aria-label="escape" aria-keyshortcuts="Escape"
      >$services.icon.renderHTML('cross')&lt;/a>
    &lt;/div>
    &lt;div class="lightboxDescription">
      &lt;div class="descriptionCol">
        &lt;span class="title">&lt;/span>
      &lt;/div>
      &lt;div class="descriptionCol">
        &lt;span class="publisher">&lt;/span>
        &lt;span class="date">&lt;/span>
       &lt;/div>
    &lt;/div>
    &lt;ol class="indicator">&lt;/ol>
  &lt;/div>
  {{/html}}
#end
{{/velocity}}</content>
