    <property>
      <code>.lightboxDescription {
  background-color: white;
  display: grid;
  grid-template-columns: auto auto;
  justify-items: center;
  align-items: center;
  position: absolute;
  font-size: 2.5vw;
  width: 100%;
  height: 5%;
  bottom: 0;
  padding: 0;
}
.descriptionCol {
  padding-left: 3%;
}
#blueimp-gallery > div.slides img {
  bottom: 15%;
}
#blueimp-gallery > .indicator {
  bottom: 10%;
}
.controls {
  position: absolute;
  top: 3%;
  right: 3%;
  opacity: .8;
  cursor: pointer;
}
.blueimp-gallery .fullscreen:hover,
.blueimp-gallery .download:hover,
.blueimp-gallery .autoPlay:hover,
.blueimp-gallery .escape:hover {
  color: white;
  opacity: 1;
}
.blueimp-gallery .fullscreen,
.blueimp-gallery .download,
.blueimp-gallery .autoPlay,
.blueimp-gallery .escape {
  display: none;
}
.blueimp-gallery-controls .fullscreen,
.blueimp-gallery-controls .download,
.blueimp-gallery-controls .autoPlay,
.blueimp-gallery-controls .escape {
  display: block;
  /* Fix z-index issues (controls behind slide element) on Android: */
  -webkit-transform: translateZ(0);
  -moz-transform: translateZ(0);
  -ms-transform: translateZ(0);
  -o-transform: translateZ(0);
  transform: translateZ(0);
}</code>
